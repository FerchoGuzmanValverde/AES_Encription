<Window x:Class="AES_Encryption.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AES_Encryption"
        mc:Ignorable="d"
        Title="MainWindow" Height="205" Width="392">
    <Grid>
        <Label x:Name="label" Content="AES Encryption" HorizontalAlignment="Center" Margin="0,10,0,0" VerticalAlignment="Top" FontWeight="Bold" FontSize="16"/>
        <Button x:Name="btnEncrypt" Content="Encrypt" HorizontalAlignment="Left" Margin="51,67,0,0" VerticalAlignment="Top" Height="39" Width="128" Click="btnEncrypt_Click"/>
        <Button x:Name="btnDecrypt" Content="Decrypt" HorizontalAlignment="Left" Margin="212,67,0,0" VerticalAlignment="Top" Height="39" Width="128" Click="btnDecrypt_Click"/>
        <Label x:Name="lblMsg" Content="Message..." HorizontalAlignment="Center" VerticalAlignment="Top" Width="288" Margin="0,131,0,0"/>

    </Grid>
</Window>
